{
  "name": "@zeit/cosmosdb-server-mock",
  "version": "0.0.1",
  "description": "A mock CosmosDB server",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "files": [
    "bin",
    "cert.pem",
    "key.pem",
    "lib"
  ],
  "bin": {
    "cosmosdb-server": "bin/cosmosdb-server.js"
  },
  "scripts": {
    "build": "flow-remove-types src/ -d lib/",
    "format": "prettier --write '{bin,src,test}/**/*.js'",
    "format-staged": "git diff --cached --name-only '*.js' | xargs prettier --list-different",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-staged": "git diff --cached --name-only '*.js' | xargs eslint",
    "prepublish": "yarn run build",
    "test": "best -I test/index.js -r flow-remove-types/register"
  },
  "dependencies": {
    "path-to-regexp": "2.2.1",
    "raw-body": "2.3.3",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@zeit/best": "0.4.3",
    "@zeit/cosmosdb": "8.3.0",
    "@zeit/git-hooks": "0.1.4",
    "babel-eslint": "8.2.6",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "flow-remove-types": "1.2.3",
    "prettier": "1.14.2"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended",
      "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "plugins": [
      "flowtype"
    ]
  },
  "git": {
    "pre-commit": [
      "lint-staged",
      "format-staged"
    ]
  }
}
